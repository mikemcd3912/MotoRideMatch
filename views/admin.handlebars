<!--Display Rider Table-->
<div class="accordion-custom">
  <div id="accordion" class="accordion">
    <center>
      <button class="btn btn-outline-dark btn-lg" data-toggle="collapse" href="#Riders">
         Riders
      </button>
      <button class="btn btn-outline-dark btn-lg" data-toggle="collapse" href="#Garages">
        Garages
      </button>
      <button class="btn btn-outline-dark btn-lg" data-toggle="collapse" href="#Bikes">
        Bikes
      </button>
      <button class="btn btn-outline-dark btn-lg" data-toggle="collapse" href="#Routes">
        Routes
      </button>
      <button class="btn btn-outline-dark btn-lg" data-toggle="collapse" href="#Tours">
        Tours
      </button>
       <button class="btn btn-outline-dark btn-lg" data-toggle="collapse" href="#Terrains">
         Terrains
      </button>
    </center>
  <div id="Riders" class="collapse" data-parent="#accordion">
    <div class="card-body">
      <div class="custom-repo">
        <h2><center>Riders</center></h2>                                           
      <div class="container" width="100%" >
        <div class="row">
        </div>
        </br>
        <table class="table">
          <thead>
          <tr>
            <th scope="col">rider_ID</th>
            <th scope="col">user_Name</th>
            <th scope="col">first_name</th>
            <th scope="col">last_name</th>
            <th scope="col">dob</th>
            <th scope="col">email</th>
            <th scope="col">city</th>
            <th scope="col">state</th>
            <th scope="col">zip</th>
            <th scope="col">Action</th>
            <th scope="col"></th>
          </tr>
          </thead>
            <tbody>
              {{#each Riders}}
              <tr>
                <!--Display info-->
                  <td>{{this.rider_ID}}</td>
                  <td>{{this.user_Name}}</td>
                  <td>{{this.first_Name}}</td>
                  <td>{{this.last_Name}}</td>
                  <td>{{this.dob}}</td>
                  <td>{{this.email}}</td>
                  <td>{{this.city}}</td>
                  <td>{{this.state}}</td>
                  <td>{{this.zip}}</td>
                  <!--Update Form-->
                  <td><button href="#Riders{{this.rider_ID}}" data-toggle="modal" class="btn btn-outline-dark btn-small">Update</button></td>
                  <div class="modal" id="Riders{{this.rider_ID}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                    <div class="modal-dialog" role="document">
                      <div class="modal-content">
                        <div class="modal-header">
                          <h5 class="modal-title">Update Rider</h5>
                        </div>
                        <div class="modal-body">
                        <div class="container" width="50%">
                  <form id="update{{this.rider_ID}}" action="/update" method="POST">
                      <div class="form-group row">
                              <input type="hidden" class="form-control" name="table" readonly="readonly" value="Riders">
                              <input type="hidden" class="form-control" name="id" readonly="readonly" value={{this.rider_ID}}>
                              <div scope='col' >First Name<input type="text" class="form-control" name="first_name" value={{this.first_Name}} size="10" ></div>
                              <div scope='col'>Last Name<input type="text" class="form-control" name="last_name" value={{this.last_Name}} size="10" ></div>
                              <div scope='col'>Date of Birth<input type="date" class="form-control" name="dob" value={{this.dob}} size="10" required></div>
                              <div scope='col'>Email<input type="text" class="form-control" name="email" value={{this.email}} size="10" ></div>
                              <div scope='col'>City<input type="text" class="form-control" name="city" value={{this.city}} size="10" ></div>
                              <div scope='col'>State<input type="text" class="form-control" name="state" value={{this.state}} size="10" ></div>
                              <div scope='col'>Zip<input type="text" class="form-control" name="zip" value={{this.zip}} size="10" ></div>
                      </div>
                  </form>
                  </div>
                        </div>
                        <div class="modal-footer">
                          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                          <button form="update{{this.rider_ID}}" type="submit" class="btn btn-primary">Update</button>
                        </div>
                      </div>
                    </div>
                  </div>
                  <!--Delete Form-->
                  <td><form action= "/adminDelete" method='POST'>
                      <input type="hidden" value="Riders" name="table">
                      <input type="hidden" value="rider_ID" name="field">
                      <input type="hidden" value={{this.rider_ID}} name="id">
                      <button type="submit" class="btn btn-outline-dark btn-small">Delete</button></form></td>
              </tr>
              {{/each}}
          </tbody>
        </table>
          <center>
            <button href="#insertRider" data-toggle="modal" class="btn btn-outline-dark btn-small">Insert</button>
          </center>
        </div>
      </div>
    </div>
  </div>

<!--Insert Rider Modal-->
