            <div scope='col'><input type="date" class="form-control" name="dob" placeholder="dob" size="10" required></div>
            <div scope='col'><input type="text" class="form-control" name="email" placeholder="email" size="10" required></div>
            <div scope='col'><input type="text" class="form-control" name="city" placeholder="city" size="10" required></div>
            <div scope='col'><input type="text" class="form-control" name="state" placeholder="state" size="10" required></div>
            <div scope='col'><input type="text" class="form-control" name="zip" placeholder="zip" size="10" required></div>
    </div>
</form>
</div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button form="addRider" type="submit" class="btn btn-primary">Insert</button>
      </div>
    </div>
  </div>
</div>

