            <th scope="col">terrain_Description</th>
            <th scope="col">Action</th>
            <th scope="col"></th>
          </tr>
          </thead>
             <tbody>
              {{#each Terrains}}
              <tr>
                  <td>{{this.terrain_ID}}</td>
                  <td>{{this.terrain_Type}}</td>
                  <td>{{this.terrain_Description}}</td>
                  <!--Update Form-->
                  <td><button href="#Terrain{{this.terrain_ID}}" data-toggle="modal" class="btn btn-outline-dark btn-small">Update</button></td>
                  <div class="modal" id="Terrain{{this.terrain_ID}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                    <div class="modal-dialog" role="document">
                      <div class="modal-content">
                        <div class="modal-header">
                          <h5 class="modal-title">Update Terrain</h5>
                        </div>
                        <div class="modal-body">
                        <div class="container" width="50%">
                  <form id="updateTerrain{{this.terrain_ID}}" action="/update" method="POST">
                      <div class="form-group row">
                              <input type="hidden" class="form-control" name="table" readonly="readonly" value="Terrains">
                              <input type="hidden" class="form-control" name="terrain_ID" readonly="readonly" value={{this.terrain_ID}}>
                              <div scope='col' >Terrain Type<input type="text" class="form-control" name="terrain_Type" value={{this.terrain_Type}} size="10" ></div>
                              <div scope='col'>Terrain Description<input type="text" class="form-control" name="terrain_Description" value={{this.terrain_Description}} size="10" ></div>
                      </div>
                  </form>
                  </div>
                        </div>
                        <div class="modal-footer">
                          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                          <button form="updateTerrain{{this.terrain_ID}}" type="submit" class="btn btn-primary">Update</button>
                        </div>
                      </div>
                    </div>
                  </div>
                  <!--Delete Form-->
                  <td><form action= "/adminDelete" method='POST'>
                      <input type="hidden" value="Terrains" name="table">
                      <input type="hidden" value="terrain_ID" name="field">
                      <input type="hidden" value={{this.terrain_ID}} name="id">
                      <button type="submit" class="btn btn-outline-dark btn-small">Delete</button></form></td>
              </tr>
              {{/each}}
          </tbody>
        </table>
          <center>
            <button href="#insertTerrain" data-toggle="modal" class="btn btn-outline-dark btn-small">Insert</button>
          </center>
        </div>
      </div>
    </div>
  </div>

  <!--Insert Terrain Modal-->
